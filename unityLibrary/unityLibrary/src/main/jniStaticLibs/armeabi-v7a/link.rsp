--target=armv7a-linux-androideabi23
-shared
-Wl,-soname,libunity.so
-Wl,--no-undefined
-Wl,--build-id
-Wl,-z,noexecstack
-Wl,-z,relro
-Wl,--gc-sections
-Wl,--no-rosegment
-fuse-ld=lld{HOST_EXE}
-Wl,--wrap,abort
"$(UNITY_LIBS)/libmain.so"
-Wl,--exclude-libs,libunityexternal.a
-stdlib=libc++
-static-libstdc++
-Wl,-u,JNI_OnLoad
-Wl,-u,JNI_OnUnload
-Wl,-u,UnitySendMessage
-Wl,-u,UnitySwappy_version
-Wl,-u,UnitySwappy_injectTracer
-Wl,-u,UnityInitialize
-Wl,-u,UnityShutdown
-Wl,--version-script="$(UNITY_COMMONSTATICLIBS)/libunity.map"
-l"android"
-l"log"
-l"z"
-l"EGL"
-l"atomic"
-l"mediandk"
-l"unwind"
